{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341/" }
      }
    ]

  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OrganisationServiceConnectionString": "Host=localhost; Port=5432; Database=BookStore; UserName=postgres; Password=1234"
  }
}
